        <div class="entry-content-wrapper clearfix"> 
         <div class="flex_column av_one_half first  avia-builder-el-27  el_before_av_one_half  avia-builder-el-first  ">
          <section class="av_textblock_section" itemscope itemtype="https://schema.org/CreativeWork">
           <div class="avia_textblock " itemprop="text"> 
            <div class="mg_grid_wrap mg_deeplink"> 
             <div id="mg_grid_105" class="mg_container lcwp_loading " rel="auto"> 
              <div id="570d4c98ed9c1" class="mg_box mg_transitions col1_4 row1_4 mg_video mg_closed mgc_cadets-4 mgc_military-4 mgc_video-2" rel="pid_2042"> 
               <div class="img_wrap"> 
                <div>
                 <img src="http://admissions.vmi.edu/wp-content/uploads/ewpt_cache/240x240_85_1_c_FFFFFF_4e700cedef579c95fc037c95c594ac53.jpg" class="thumb" alt="New Cadet Oath Ceremony"> 
                 <div class="overlays"> 
                  <div class="overlay"></div> 
                  <div class="cell_more">
                   <span></span>
                  </div> 
                  <div class="cell_type">
                   <span class="mg_overlay_tit">New Cadet Oath Ceremony</span>
                  </div> 
                 </div>
                </div> 
               </div> 
              </div> 
              <div id="570d4c98ee99c" class="mg_box mg_transitions col1_4 row1_4 mg_image mg_closed mgc_photo" rel="pid_1903"> 
               <div class="img_wrap"> 
                <div>
                 <img src="http://admissions.vmi.edu/wp-content/uploads/ewpt_cache/240x240_85_1_c_FFFFFF_e19e9daafd1b425e3e2801b5d611c3a5.jpg" class="thumb" alt="Academics"> 
                 <div class="overlays"> 
                  <div class="overlay"></div> 
                  <div class="cell_more">
                   <span></span>
