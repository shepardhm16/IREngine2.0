                        this.naturalWidth === 0 ) 
                        || this.readyState === 'uninitialized' ) {
                        $(this).attr('src', '/thumbnail/'+$(this).data('id'));
                        console.log('broken:'+$(this).data('id'));
                    }
                });
            });
            if ($('#statistaLogo').css('width') == '130px') {
    
    $('#navigation').on('click','#showSlideIn a',function(event) {
        //alert('showMenu');
        var menu = $('#navigation ul.menu');
        if (menu.hasClass('hide')) {
            menu.animate( { opacity: "1" }, { queue: false, duration: 400 }).slideDown(400, function(){
                menu.removeClass('hide');
            });
        } else {
            menu.animate( { opacity: "0" }, { queue: false, duration: 400 }).slideUp(400, function(){
                menu.addClass('hide');
            });
        }
    });

        $('#footer .span2 ul').addClass('hide');
    $('#footer .span2').on('click',function() {
        var footerSub = $(this).find('ul');
        if (footerSub.hasClass('hide')) {
            footerSub.animate( { opacity: "1" }, { queue: false, duration: 400 }).slideDown(400, function(){
                footerSub.removeClass('hide');
            });
        } else {
            footerSub.animate( { opacity: "0" }, { queue: false, duration: 400 }).slideUp(400, function(){
                footerSub.addClass('hide');
            });
        }
    });
    
    $('#searchResults ul.statListNew li').each(function() {
        // add img tags for lazy loading content
        var curEntity = $(this).data('entity');
        if (curEntity == 'statistics' || curEntity == 'infographics' || curEntity == 'topics') {
            var curIcon = $(this).find('.sprite');
            
            var preview = curIcon.before('<img src="'+$(this).data('thumb')+'" width="100" height="71" class="floatLeft prevImg"/>');
            curIcon.css('position','absolute').css('left','82px').css('top','10px');
        }
    });
    
    // create searchbar
    $('#logo').prepend('<div class="floatRight searchbar"><a href="https\x3A\x2F\x2Fwww.statista.com\x2Fsearch\x2F"><span class="front">Search&hellip;</span><span class="back"><span class="searchicon" style="background-image: url(http://statistacdn.s3.amazonaws.com/static/icons/search-icon-white.png);background-size: 15px 15px;width: 15px; height: 15px;display: inline-block;position: relative;top: 6px;left: 7px;">&nbsp;</span></span></a></div>');

    // hide search filter
    $('#searchBar .filterForm').addClass('hide');
    $('#searchBar').append($('#searchAreasBox'));
    $('#searchBar #searchAreasBox').addClass('hide');
    $('#searchBar').append('<span class="button" id="showFiltersMobile"><span class="sprite settings-grey" style="margin-right: 7px;"></span>Show Filters</span>');
    
    $('#searchBar').on('click','#showFiltersMobile',function() {
        var filterForm = $('#searchBar .filterForm, #searchBar #searchAreasBox');
        if (filterForm.hasClass('hide')) {
            filterForm.animate( { opacity: "1" }, { queue: false, duration: 400 }).slideDown(400, function(){
                filterForm.removeClass('hide');
            });
        } else {
            filterForm.animate( { opacity: "0" }, { queue: false, duration: 400 }).slideUp(400, function(){
                filterForm.addClass('hide');
            });
        }
    });
    
