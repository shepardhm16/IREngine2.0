            function getCookie(name) {
                var nameEQ = name + "=";
                var ca = document.cookie.split(';');
                for(var i=0;i < ca.length;i++) {
                    var c = ca[i];
                    while (c.charAt(0)==' ') c = c.substring(1,c.length);
                    if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
                }
                return null;
            }

            function createCookie(name, value, days) {
                var expires;
                if (days) {
                    var date = new Date();
                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                    expires = "; expires=" + date.toGMTString();
                }
                else {
                    expires = "";
                }
                document.cookie = name + "=" + value + expires + "; path=/";
            }

            $(document).ready(function(){  
                $("#q,#mainq,.search-autocomplete").autocomplete({
                    appendTo: "#ac",
                    dataType: "json",
                    minLength: 3,
                    delay: 300,
                    sortResults: false,
                    source: "/autocomplete/",
                    open: function(event, ui) {
                        $(".ui-autocomplete").css("z-index", 1000);
                    },
                    select: function( event, ui ) {
                        var url = ui.item.label;
                        if(url != '#') {
                            window.location.href = "https://www.statista.com/search/?q=" + url;
                        }
                    }
                });
                                var oldFn = $.ui.autocomplete.prototype._renderItem;

                $.ui.autocomplete.prototype._renderItem = function( ul, item) {
                    var re = new RegExp("(" + this.term + ")", "ig") ;
                    var t = item.label.replace(re,"<span class='autocompleteHighlightItem'>" + 
                            "$&" + "</span>");
                    return $( "<li></li>" )
                        .data( "item.autocomplete", item )
                        .append( "<a>" + t + "</a>" )
                        .appendTo( ul );
                };
                $('#navigation .menu ul.menu li').hover(function(event) {
                                        $(this).addClass('js-hover').delay(180).queue(function(){
                        $(this).removeClass("js-hover").dequeue();
                    });
                }, function(event) {
                    $(this).removeClass('js-hover');
                });                
                                
                //inview animation
$('#topicinfoboxImg').bind('inview', function (event, visible, visiblePartX, visiblePartY) {
    if (visible == true) {
        // element is now visible in the viewport
        $(this).removeClass('invisible').find('div.animated').addClass('fadeInUp');
    } else {
        $(this).removeClass('');
    }
});
                
                                    
                                                $('.youtubeLayer').removeClass('hideNoscript');
            });

            $(window).on('load',function() {
                $('img.tn').each(function() {
