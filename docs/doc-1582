  <!-- Dynamic DataLayer -->
  <script type="text/javascript">
            
$('[data-promoclick]').on('click',function(e) {
    e.preventDefault();
    var url = $(this).attr('href');
    
    var promoview = $('body').data('promoview');
    
    console.log('PromoClick triggered');
    console.dir(promoview);

    dataLayer.push({ // (DYNAMIC)
        'event': 'promoClick',
        'eventCallback': function() {
            console.log('Redirecting');
            document.location = url;
        },
        'ecommerce': {
            'promoClick': {
                'promotions': [
                {
                     'id': promoview.id,
                     'name': promoview.name,
                     'creative':promoview.creative,
                     'position': promoview.pos
                }]
            }
        }
    });
});
                                                            
