  <script type="text/javascript">
    $(document).ready(function(){
      if(cdm.gvars.internetExplorerVersion < IE_8_VERSION){
        if($("#details").exists() ){
          setTimeout("showtheDetailsIE7()",1000);
        }
      }
    });
    function showtheDetailsIE7(){
      $("#details").css("display","block");
    }
