function FrontPage_Form1_Validator(theForm)
{

  if (theForm.LastName.value == "")
  {
    alert("Please enter a value for the \"LastName\" field.");
    theForm.LastName.focus();
    return (false);
  }

  if (theForm.LastName.value.length < 1)
  {
    alert("Please enter at least 1 characters in the \"LastName\" field.");
    theForm.LastName.focus();
    return (false);
  }

  if (theForm.LastName.value.length > 40)
  {
    alert("Please enter at most 40 characters in the \"LastName\" field.");
    theForm.LastName.focus();
    return (false);
  }

  var checkOK = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyzƒŠŒŽšœžŸÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ";
  var checkStr = theForm.LastName.value;
  var allValid = true;
  var validGroups = true;
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
  }
  if (!allValid)
  {
    alert("Please enter only letter characters in the \"LastName\" field.");
    theForm.LastName.focus();
    return (false);
  }

  if (theForm.FirstName.value == "")
  {
    alert("Please enter a value for the \"FirstName\" field.");
    theForm.FirstName.focus();
    return (false);
  }

  if (theForm.FirstName.value.length < 1)
  {
    alert("Please enter at least 1 characters in the \"FirstName\" field.");
    theForm.FirstName.focus();
    return (false);
  }

  if (theForm.FirstName.value.length > 40)
  {
    alert("Please enter at most 40 characters in the \"FirstName\" field.");
    theForm.FirstName.focus();
    return (false);
  }

  var checkOK = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyzƒŠŒŽšœžŸÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ";
  var checkStr = theForm.FirstName.value;
  var allValid = true;
  var validGroups = true;
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
  }
  if (!allValid)
  {
    alert("Please enter only letter characters in the \"FirstName\" field.");
    theForm.FirstName.focus();
    return (false);
  }

  if (theForm.Classof.value == "")
  {
    alert("Please enter a value for the \"Classof\" field.");
    theForm.Classof.focus();
    return (false);
  }

  if (theForm.Classof.value.length < 4)
  {
    alert("Please enter at least 4 characters in the \"Classof\" field.");
    theForm.Classof.focus();
    return (false);
  }

  if (theForm.Classof.value.length > 4)
  {
    alert("Please enter at most 4 characters in the \"Classof\" field.");
    theForm.Classof.focus();
    return (false);
  }

  var checkOK = "0123456789-";
  var checkStr = theForm.Classof.value;
  var allValid = true;
  var validGroups = true;
  var decPoints = 0;
  var allNum = "";
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
    allNum += ch;
  }
  if (!allValid)
  {
    alert("Please enter only digit characters in the \"Classof\" field.");
    theForm.Classof.focus();
    return (false);
  }

  if (theForm.DateofGraduation.value == "")
  {
    alert("Please enter a value for the \"DateofGraduation\" field.");
    theForm.DateofGraduation.focus();
    return (false);
  }

  if (theForm.DateofGraduation.value.length < 4)
  {
    alert("Please enter at least 4 characters in the \"DateofGraduation\" field.");
    theForm.DateofGraduation.focus();
    return (false);
  }

  if (theForm.DateofGraduation.value.length > 4)
  {
    alert("Please enter at most 4 characters in the \"DateofGraduation\" field.");
    theForm.DateofGraduation.focus();
    return (false);
  }

  var checkOK = "0123456789-";
  var checkStr = theForm.DateofGraduation.value;
  var allValid = true;
  var validGroups = true;
  var decPoints = 0;
  var allNum = "";
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
      allValid = false;
      break;
    }
    allNum += ch;
  }
  if (!allValid)
  {
    alert("Please enter only digit characters in the \"DateofGraduation\" field.");
    theForm.DateofGraduation.focus();
    return (false);
  }

  var checkOK = "0123456789-";
  var checkStr = theForm.YrsActiveDuty.value;
  var allValid = true;
  var validGroups = true;
  var decPoints = 0;
  var allNum = "";
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
