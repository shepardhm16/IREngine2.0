//<![CDATA[
function CloseCategorySelect(ShowCategory, call_number)
{
var editor = document.getElementById('ek_forum_editor');
var category  = document.getElementById('ek_category');
if( call_number == null ) { call_number = 0; }
if( ShowCategory ) {
    editor.style.display = 'none'; 
    category.style.display = 'inline';
    category.style.width = '570px';
    category.style.height = '410px';
}
else {
   editor.style.display = 'inline';
   category.style.display = 'none';
}
}
    function ek_ShowPane(pane) {
        var dvContent = document.getElementById('ek_dvContent');
        var dvCategory = document.getElementById('ek_dvCategory');
        if( dvContent == null || dvCategory == null ) { return; }
        if( pane == 'content' ) { dvContent.className = 'ek_tab_activated'; dvCategory.className = 'ek_tab_disabled'; }
        else { dvContent.className = 'ek_tab_disabled'; dvCategory.className = 'ek_tab_activated'; }
if( pane == 'content')
{
if( document.getElementById('ek_category') ) {
    document.getElementById('ek_category').contentWindow.SaveCategory();
}
}
else {
CloseCategorySelect(true);
}
}
//]]>
