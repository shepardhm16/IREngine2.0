        $("#hicKeyword").autocomplete("http://www.mergentintellect.com/index.php/advancesearch/autocompleteHickeyword", {
            width: 272,
            matchContains: true,
            selectFirst: false,
            minChars:2, 
            autoFill:false,
            mustMatch:false,
            cacheLength:10,
            max:10
        });
                    
        $('#sicKeyword,#peoplesicKeyword').result(function (event, data, formatted) {
            var currentid = event.target.id;
            var sichtml = '';
            sichtml = '<table cellspacing=0 cellpadding=0 border=0 class=data_grid_criteria width=100% style="margin:30px 0px;">';
            sichtml += '<tr><th align="center">SIC Code</th><th align="center">SIC Description</th><th align="center">Action</th></tr>';
      
            $('#'+currentid).val(''); 
            var sicKeywordcodes= $('#'+currentid+'codes').val();
            if(sicKeywordcodes == '') {
                $('#'+currentid+'codes').val(data).trigger('change');
            } else if(sicKeywordcodes != '') {
                var sicKeywordcodesoldval = $('#'+currentid+'codes').val();
                var sicKeywordcodesnewval = sicKeywordcodesoldval +"#"+data;
                $('#'+currentid+'codes').val(sicKeywordcodesnewval).trigger('change');
            }
            var sicKeywordcodesnew= $('#'+currentid+'codes').val();
            var sicKeywordcodesnew_val = sicKeywordcodesnew.split("#");
            for(var i=0;i< sicKeywordcodesnew_val.length;i++){
                if(i%2==0){ var colorBg = '#f5f5f5'; } else { var colorBg = '';}
                var sicKeywordcodesnew_value = sicKeywordcodesnew_val[i].split("  ");
                //alert(sicKeywordcodesnew_value[1]);
                sichtml += "<tr style='background-color:"+colorBg+";'><td align='center' >"+sicKeywordcodesnew_value[0]+ "</td><td align='center'>"+sicKeywordcodesnew_value[1]+ "</td><td align='center'><a style='top:0px;left:0px;' href='#' onclick='removeSiccodes("+i+",\""+currentid+"\")'>Delete</a></td></tr>";
            }
                        
            sichtml += '</table>'
            $('#'+currentid+'codes_display').html(sichtml);
            
        });
        
        $('#hicKeyword').result(function (event, data, formatted) {
            var hichtml = '';
            hichtml = '<table cellspacing=0 cellpadding=0 border=0 class=data_grid width=100% style="margin:30px 0px;">';
            hichtml += '<tr><th align="center">HIC Code</th><th align="center">HIC Name</th><th align="center">Action</th></tr>';
      
