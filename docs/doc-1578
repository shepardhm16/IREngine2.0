        $('#naiccode_keyword').val(naiccode_keyword_final);
  
    }
    
    function storeCheckedvalues_advancesearch(dunsid,status) {
        var searchtype = $("#buildfilesfor").val();
        if (typeof searchtype == undefined) {
            searchtype = "";
        }
        //alert(dunsid);
        //alert(status);
        jQuery('#loading-image').show();
        var dunsid_count = $('#checkeddunsid').val().split(",");
        var rangeRecordsConfig = "2000";
        
        var dunsid_count_final = parseInt(dunsid_count.length);
        
         var maxRecordslimit = "";
                if(parseInt(maxRecordslimit) < parseInt(rangeRecordsConfig)){
                    var rangeRecordsConfig_final = maxRecordslimit;
                    var recordlimit_max = 1;
                } else {
                    var rangeRecordsConfig_final = rangeRecordsConfig;
                    var recordlimit_max = 2;
                }
        //alert(rangeRecordsConfig_final);
        
        if((parseInt(dunsid_count_final) < parseInt(rangeRecordsConfig_final)) || status == false){
            var checkedrecords;
            $.ajax({
                type: 'POST',
                url: 'http://www.mergentintellect.com/index.php/advancesearch/saveCheckboxvalues/'+dunsid+'/'+status+'/'+searchtype,
                dataType: 'json',
                //data: templateid,
                success: function(data) {
                    jQuery('#loading-image').hide();

                    //alert(data['status']);
                    //alert(data['checkedrecords']);
                    //code for Al select checkbox checked status Starts
                    var dbdunsid = $('#dbdunsid').val().split(",");
                    var allchkstatus = new Array();
                    for(i=0;i<dbdunsid.length;i++) {
                        if ($('#chk'+dbdunsid[i]).attr('checked')) {
                            allchkstatus[i] = 1;
                        } else {
                            allchkstatus[i] = 0; 
                        }
                    }
                    var checkvalue = include(allchkstatus, 0);
                    if(checkvalue == undefined) {
                        setCustomCheckbox($("#toggleChecked"), true);
                    } else {
                        setCustomCheckbox($("#toggleChecked"), false);
                    }  
                    //code for Al select checkbox checked status Ends
                    $("#checkeddunsid").val(data['checkedrecords']);
                    if($("#checkeddunsid").val() == '') {
                        checkedrecords = ""; 
                    } else {
                        checkedrecords = $("#checkeddunsid").val();
                    }

                    if(checkedrecords != '') {
                        var checkedrecordsSplit = checkedrecords.split(",");
                        $("#checkedcount").html(checkedrecordsSplit.length);
                    } else {
                        $("#checkedcount").html("0"); 
                    }
                }
            });
        } else {
            if(recordlimit_max == 2){
                alert("The maximum no Records in a single Download should be less than or equal to "+rangeRecordsConfig_final);
            } else {
                alert("The maximum no Records should be less than or equal to "+rangeRecordsConfig_final);    
            }
            setCustomCheckbox($("#chk"+dunsid), false);
        }
    }
    
    function include(arr, obj) {
        for(var i=0; i<arr.length; i++) {
            if (arr[i] == obj) return true;
        }
    }
    
    function buildFilesRange() {
        $("#lightboxprofile, #lightbox-profile").fadeIn(300); 
    }
    
    function exportResults(searchpage) {
        $("#searchpage").val(searchpage)
        $("#lightboxprofile, #lightbox-exportresult").fadeIn(300); 
    }
    
