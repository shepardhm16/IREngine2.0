                naichtml += "<tr style='background-color:"+colorBg+";'><td align='center'>"+naicKeywordcodesnew_value[0]+ "</td><td align='center'>"+naicKeywordcodesnew_value[1]+ "</td><td align='center'><a style='top:0px;left:0px;' href='#' onclick='removeNaiccodes("+i+",\""+currentid+"\")'>Delete</a></td></tr>";
