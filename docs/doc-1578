    function Chartareamultiple(title, path, refineval, chartTitle){
        if (typeof chartTitle == undefined) {
            chartTitle = title;
        }
        $.blockUI();
        $.ajax({
            type: 'POST',
            url: 'http://www.mergentintellect.com/index.php'+path+refineval,
            dataType: 'json',
            success: function(data) {
                $elem = $('#'+title);
                $elem.parent().parent().find('li').each(function(index, el) {
                    $(el).removeAttr('class').children('a').removeAttr('class');
                });
                $elem.attr('class', 'active').parent().attr('class', 'active');

                $("#salesfiltergraphHTML").html(data['html']);
                $.unblockUI(); 
                drawChart(chartTitle, data['chart_values']);
            }
        });
    }

    function Chartareacountry(refineval){
        Chartareamultiple('Country', "/advancesearch/generatechartcountry/", refineval, 'Country');
    }

    function Chartareaindustry(module,refine){
        Chartareamultiple('Industry', "/advancesearch/generatechartindustry/", module+'/'+refine, 'Industry');
    }
    
