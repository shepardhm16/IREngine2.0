            var baseUrl = "http://www.mergentintellect.com/";
            function printThis(winName, winSpec, title, cssArray, data, autoload) {
                var mywindow = window.open('', winName, 'scrollbars=yes,'+winSpec);
                mywindow.document.write('<html><head><title>'+title+'</title>');	
                var cssLength = cssArray.length;
                for (var i = 0; i < cssLength; i++) {
                    mywindow.document.write('<link rel="stylesheet" href="'+cssArray[i]+'" type="text/css" />');
                }
                mywindow.document.write('<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js" type="text/javascript" charset="utf-8"><\/script>');
				

                if(autoload != "off"){
					mywindow.document.write('</head><body onload="printMe();">');
				}else{
					mywindow.document.write('</head><body>');
				}
				
                mywindow.document.write(data);
                mywindow.document.write('<script>');


				if(autoload != "off"){
					mywindow.document.write('function printMe() {');
				}
				
                mywindow.document.write('var iframe = document.getElementsByTagName(\'\iframe\'\);');
                mywindow.document.write('if(iframe.length>0) {');
                mywindow.document.write('setTimeout(function() {');
                mywindow.document.write('window.print();');
                mywindow.document.write('}, 6000);');
                mywindow.document.write('}else{');
//                 mywindow.document.write('window.print()');              
                mywindow.document.write('setTimeout(function() {');
                mywindow.document.write('window.print();');
                mywindow.document.write('}, 1500);');
                
                if(autoload != "off"){
                	mywindow.document.write('}');
				}
                mywindow.document.write('}');
                mywindow.document.write('<\/script>');
                mywindow.document.write('</body></html>'); 
                mywindow.document.close(); 
                mywindow.focus();  
                return true;
            }

            function PrintingElem(elem)
            {
                var cssArray = [
                    "http://www.mergentintellect.com/assets/css/global.css", 
                    "http://www.mergentintellect.com/assets/css/developer.css",
                    "http://www.mergentintellect.com/assets/css/keyexecutives.css",
                    "http://www.mergentintellect.com/assets/css/print.css"];

                var data = $(elem).html();
                var disclaimer = $('#print_disclaimer').html();
                if (disclaimer !== null) {
                    data += '<br clear="all" /><div class="disclaimer">'+disclaimer+'</div>';
                }
                
                var autoload = 'on';
                
                if ( data.indexOf('googleMapPinScript') !== -1 ) {
//                 	data = data.replace('<script src="http://maps.gstatic.com/maps-api-v3/api/js/20/11c/main.js"><\/script>','');

                	var startPos = data.indexOf('<script src="http://maps.gstatic.com');
                	var endPos = data.indexOf('main.js"><\/script>');

                	endPos = endPos + 19;
                	
                	var stringToRemove = data.substring(startPos, endPos);

                	data = data.replace(stringToRemove,'');
                	
                	autoload = 'off';      
                }

                printThis('print', 'height=800,width=956', 'Company Details', cssArray, data, autoload);
            }

            function printScreen(elem)
            {   
                var cssArray = ["http://www.mergentintellect.com/assets/css/print.css"];
                printThis('print', 'height=500,width=700', 'Company Details', cssArray, $("#"+elem).html());
            }

            function changeaction(link, recordperpage, delimiter)
            {
                newpage = document.pager.page.value;
                newurl = "http://www.mergentintellect.com/"+link+delimiter+"start="+newpage+"&perpage="+recordperpage;
                document.pager.action = newurl;
                document.pager.submit();
            }

            function openNAICCodes() {
                $("#lightboxprofile, #lightbox-SIC").fadeOut(300);
                $("#lightboxprofile, #lightbox-NAIC").fadeIn(300);
            }
            function openSICCodes() {
                $("#lightboxprofile, #lightbox-NAIC").fadeOut(300);
                $("#lightboxprofile, #lightbox-SIC").fadeIn(300);
            }

            function closeNAICCodes() {
                $("#lightboxprofile, #lightbox-NAIC").fadeOut(300);
            }
            function closeSICCodes() {
                $("#lightboxprofile, #lightbox-SIC").fadeOut(300);
            }

            function redirectPage(state) {
                window.location.href= baseUrl+'index.php/search/quick?state='+state;
            }

            // adding sort features
            function addSorting(elem) {
                var curr_sort_field = "";
                var curr_sort_type = "";
                $(elem).find('th.sorttable').each(function(index, el) {
                    var title = '<div style="display: inline-block;">'+$(el).html()+'</div>';
                    var paging = '<div class="sorting" style="display: inline-block; width: 12px; padding-left: 5px; height: 10px;"><div style="padding: 1px;" class="arrow_up_active"></div><div style="clear:both;"></div><div style="padding: 1px;" class="arrow_down_active"></div></div>';
                    if (typeof $(el).attr('sorttype') !== "undefined" && $(el).attr('sorttype') != "") {
                        curr_sort_type = $(el).attr('sorttype');
                        curr_sort_field = $(el).attr('sortfield');

                        if ($(el).attr('sorttype') == 'ASC') {
                            paging = '<div class="sorting" style="display: inline-block; width: 12px; padding-left: 5px; height: 10px;"><div style="padding: 1px; margin-top: 20%;" class="arrow_up_active"></div></div>';
                        }
                        else {
                            paging = '<div class="sorting" style="display: inline-block; width: 12px; padding-left: 5px; height: 10px;"><div style="padding: 1px; margin-top: 20%;" class="arrow_down_active"></div></div>';
                        }
                    }
                    $(el).html(title+paging).css({cursor: 'pointer'}).click(function(){
                        sort_field = $(el).attr('sortfield');
                        sort_type = 'ASC';
                        if (typeof $(el).attr('sorttype') !== "undefined" && $(el).attr('sorttype') != "") {
                            if ($(el).attr('sorttype') == 'ASC') {
                                sort_type = "DESC";
                            }
                        }

                        param = '';
                        url = document.URL;
                        if (url.charAt(url.length - 1) == '#') {
                            url = url.substr(0, url.length - 1);
                        }
                        url = url.split('?');
                        if (url.length > 1) {
                            // param = org_url.split('?')[1];
                            param_obj = url[1].split('&')
                            for (i in param_obj) {
                                tmp_obj = param_obj[i].split("=");
                                if (tmp_obj[0] == "rpp" || tmp_obj[0] == "start") {                                    
                                    param+= '&' + param_obj[i];
                                }
                            };
                            url = url[0];
                        }
                        else {
                            url = url[0];
                        }
                        url+= '?sorttype='+sort_type+'&sortfield='+sort_field+param;
                        // console.log(url);
                        window.location = url;
                    });
                });

                if (curr_sort_field != "") {
                    $("#frmSelectdownloadviewresults").append('<input type="hidden" name="sorttype" value="'+curr_sort_type+'" /><input type="hidden" name="sortfield" value="'+curr_sort_field+'" />')
                }
            }

            //add dropdown sort
            function customSortDropdown(dropdownId){
                var ddlWrapId = '#'+dropdownId+'_wrap';
                var dropdownId = '#'+dropdownId;

                $(document).on("click",ddlWrapId,function(){
                    $(dropdownId).slideToggle();
                });
                
                $(document).on("click",ddlWrapId+" ul li",function(){
                    sort_field = $(this).attr('sortfield');
                    sort_type = $(this).attr('sorttype');

                    param = '';
                    url = document.URL;
                    if (url.charAt(url.length - 1) == '#') {
                        url = url.substr(0, url.length - 1);
                    }
                    url = url.split('?');
                    if (url.length > 1) {
                        // param = org_url.split('?')[1];
                        param_obj = url[1].split('&')
                        for (i in param_obj) {
                            tmp_obj = param_obj[i].split("=");
                            if (tmp_obj[0] == "rpp" || tmp_obj[0] == "start") {                                    
                                param+= '&' + param_obj[i];
                            }
                        };
                        url = url[0];
                    }
                    else {
                        url = url[0];
                    }
                    url+= '?sorttype='+sort_type+'&sortfield='+sort_field+param;
                    // console.log(url);
                    window.location = url;
                });

                $('body').click(function(e){
                    var clickedOn = $(e.target);
                    if (!clickedOn.parents().andSelf().is(ddlWrapId)){
                        $(dropdownId).hide('200');
                    }
                });
            }

            // adding zoom css fix
            function addCSSStyle(style) {
                var styleEl = document.getElementsByTagName('style'), styleSheet;
                if (styleEl.length == 0) {
                    var styleEl = document.createElement('style');
                    styleEl.title = "zoommediafix";
                    document.head.appendChild(styleEl);                                    
                    styleSheet = styleEl.sheet;
                }
                else {
                    for (var i = 0; i < styleEl.length; i++) {
                        if (styleEl[i].title == "zoommediafix") {
                            styleSheet = styleEl[i].sheet;
                        }
                    }
                }

                if (typeof styleSheet != "undefined") {
                    // CSS to fix change company live search
                    if (styleSheet.cssRules.length > 0) {
                        for (var i = 0; i < styleSheet.cssRules.length; i++) {
                            styleSheet.deleteRule(i);
                        }
                    }

                    for (var i = 0; i < style.length; i++) {
                        styleSheet.insertRule(style[i], i);
                    }
                }
            }

            // zoom the screen
            function zoomScreen(zoomType, zoomOpt) {
                if (zoomType != "") {
                    $small.removeClass('font_icon_small_active');
                    $normal.removeClass('font_icon_medium_active');            
                    $large.removeClass('font_icon_large_active');
                    $container = $(".nxmaincontainer");
                    if (zoomType == "reduce") {                            
                        $container.removeClass('reduce enlarge').addClass(zoomType);                            
                        $small.addClass("font_icon_small_active");
                    } else if (zoomType == "normal") {
                        $container.removeClass('reduce enlarge');
                        $normal.addClass("font_icon_medium_active");
                    } else {
                        $container.removeClass('reduce enlarge').addClass(zoomType);                            
                        $large.addClass("font_icon_large_active");
                    }
                }                    
            }

      $(document).on("click",".company_details_header_wrap[name='expandable'], .job_header_wrap, .expandable_header_wrap",function(){
          if($(this).children('div:first').hasClass("sec_arrow_up")){
              $(this).children('div:first').removeClass("sec_arrow_up");
                $(this).children('div:first').addClass("sec_arrow_down");
                $(this).children('div:first').prev('span').removeClass("focus");
                $(this).children('div:first').prev('span').addClass("unfocus");
                $(this).children('div:first').prev('span').find('span').removeClass("focus");
                $(this).children('div:first').prev('span').find('span').addClass("unfocus");
            }else{
                $(this).children('div:first').removeClass("sec_arrow_down");
                $(this).children('div:first').addClass("sec_arrow_up");
                $(this).children('div:first').prev('span').removeClass("unfocus");
                $(this).children('div:first').prev('span').addClass("focus");
                $(this).children('div:first').prev('span').find('span').removeClass("unfocus");
                $(this).children('div:first').prev('span').find('span').addClass("focus");
            }
            
            $(this).next('div').slideToggle();
          });
              
            $(document).ready(function(){
                jQuery_1_11('.tooltip').jBox('Tooltip', {
                    position: {
                        x: 'center',
                        y: 'bottom'
                    },
                    theme: 'TooltipDark',
                    maxWidth: 720
                });

                $sorttable = $("#sorttable");
                if ($sorttable.length > 0) {
                    addSorting($sorttable);
                }

                // Zoom binding zoom action
                $small = $("#font_icon1");
                if ($small.length == 1) {                        
                    var zoomOpt = 'company';
                                        var zoomType = '';
                                        

                    $normal = $("#font_icon2");
                    $large = $("#font_icon3");
                    $small.on('click', function(event) {
                        $.get("http://www.mergentintellect.com/index.php/search/setZoomType/"+zoomOpt+"/reduce");
                        $small.addClass("font_icon_small_active");
                        $normal.removeClass('font_icon_medium_active');
                        $large.removeClass('font_icon_large_active');
                        zoomScreen('reduce', zoomOpt);
                        event.preventDefault();
                    });

                    if ($normal.length == 1) {
                        $normal.on('click', function(event) {
                            $.get("http://www.mergentintellect.com/index.php/search/setZoomType/"+zoomOpt+"/normal");
                            $normal.addClass("font_icon_medium_active");
                            $small.removeClass('font_icon_small_active');
                            $large.removeClass('font_icon_large_active');
                            zoomScreen('normal', zoomOpt);
                            event.preventDefault();
                        });
                    }

                    if ($large.length == 1) {
                        $large.on('click', function(event) {
                            $.get("http://www.mergentintellect.com/index.php/search/setZoomType/"+zoomOpt+"/enlarge");
                            $large.addClass("font_icon_large_active");
                            $small.removeClass('font_icon_small_active');
                            $normal.removeClass('font_icon_medium_active');
                            zoomScreen('enlarge');
                            event.preventDefault();
                        });
                    }

                    zoomScreen(zoomType, zoomOpt);
                }


                $("input[name=sicnaic]:radio").click(function(){
                    $('.sicnaiccode').val('');
                    $('.sicnaiccode').focus();

                    $('#sicKeywordcodes_display').hide('');
                    $('#naicKeywordcodes_display').hide('');
                    $('#sicKeywordcodes').val('');
                    $('#naicKeywordcodes').val('');
                    $('#siccode_keyword').val('');
                    $('#naiccode_keyword').val('');

                    var sicnaic = $("input[name='sicnaic']:radio:checked").val();
                    //alert(sicnaic);
                    if(sicnaic == "naic"){
                        $('#browsesic').hide();
                        $('#browsenaic').show();

                        $('#naic_searchtype').show();
                        $('#sic_searchtype').hide();

                        $('#naictype:first').attr('checked', true);
                        $('#hiddenid').val('sicnaiccode');

                        $('#sicnaiccode_inputdiv').show();
                        $('#sicKeyword_inputdiv').hide();
                        $('#naicKeyword_inputdiv').hide();
                    } else {
                        $('#browsenaic').hide();
                        $('#browsesic').show();

                        $('#sic_searchtype').show();
                        $('#naic_searchtype').hide();

                        $('#sictype:first').attr('checked', true);
                        $('#hiddenid').val('sicnaiccode');

                        $('#sicnaiccode_inputdiv').show();
                        $('#sicKeyword_inputdiv').hide();
                        $('#naicKeyword_inputdiv').hide();
                    }
                    var data = 'sicnaic=' + sicnaic;
                    $.ajax({
                        type: 'POST',
                        url: 'http://www.mergentintellect.com/index.php/search/setRadioOptionSic',
                        dataType: 'json',
                        data: data,
                        success: function(data) {
                        }
                    });
                });

                $.validator.addMethod("alphabets", function(value, element) {
                    return this.optional(element) || /^[a-z0-9\_,() ]+$/i.test(value);
                }, "");

                $('#closeRef1').click(function(){
                    alert('test');
                    $("#lightboxprofile, #lightbox-NAIC").fadeOut(300);
                });

                $('#closeRef2').click(function(){
                    $("#lightboxprofile, #lightbox-SIC").fadeOut(300);
                });

                $("#txtcity, #txtpeoplecity").autocomplete("http://www.mergentintellect.com/index.php/search/autocompleteCity", {
                    width: 260,
                    matchContains: false,
                    selectFirst: false,
                    minChars:3,
                    autoFill:false,
                    mustMatch:false,
                    cacheLength:10,
                    max:10,
                    multiple:true
                });

                $('#location').click(function(){
                    var callAjax = $('#locationHTML').is(":empty");
                    if(callAjax){
                        $('#locationHTML').html("<div id='filter'><img src='http://www.mergentintellect.com/assets/images/loading.gif' />Loading...</div>");
                        $.ajax({
                            type: 'POST',
                            url: 'http://www.mergentintellect.com/index.php/search/getStateFilter',
                            data: 'location',
                            success: function(message) {
                                $('#locationHTML').html('');
                                $('#locationHTML').html(message);
                            }
                        });
                    } else {
                        return true;
                    }
                });

                $('#salesFilter').click(function(){
                    var callAjaxSales = $('#salesfilterHTML').is(":empty");
                    if(callAjaxSales){
