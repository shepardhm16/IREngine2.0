    }
    
    function buildFilesrangeForm(){
        var validator = $('#frmSelectdownloadrecordtypes').validate({
            errorPlacement: function(error, element) {
                if (element.attr("name") == "rangefrom") {
                  error.appendTo("#fromrangeMessage");
                } else {
                  error.appendTo("#torangeMessage");
                }
            },
            errorClass: "errors2"
        });
        if(validator == false){
            return false;
        } 
    }
    
    function exportViewResultForm(cmpeople){
        if(cmpeople == "People"){
            var companyname_arr  = $('#companyname_arr').val();
            var name_arr  = $('#name_arr').val();
            var title_arr  = $('#title_arr').val();
            var gender_arr  = $('#gender_arr').val();
            var ceo_arr  = $('#ceo_arr').val();
            
            $('#export_companyname_arr').val(companyname_arr);
            $('#export_name_arr').val(name_arr);
            $('#export_title_arr').val(title_arr);
            $('#export_gender_arr').val(gender_arr);
            $('#export_ceo_arr').val(ceo_arr);
        } else {
            var dunsid  = $('#dbdunsid').val();
            $('#exportdunsid').val(dunsid);
        }
        $("#lightboxprofile, #lightbox-exportresult").fadeOut(300);
    }
    
    function mycompRangeOnlyValidation(){
       
        if(parseInt($('#fromrange').val()) >= parseInt($('#torange').val()) ){
            //alert("To Records Should be Greater than From Records");
            return false;
        } else {
            return true;
        }
    } 
    
    function mycompRangeDiffValidation(){
        var rangeRecordsConfig = "2000";
        var maxRecordslimit = "";
        //var rangediff = (parseInt($('#torange').val()) - parseInt($('#fromrange').val()));
        var rangediff = (parseInt($('#torange').val()) - parseInt($('#fromrange').val()))+1;
        if(parseInt(maxRecordslimit) < parseInt(rangeRecordsConfig)){
            var rangeRecordsConfig_final = maxRecordslimit;
        } else {
            var rangeRecordsConfig_final = rangeRecordsConfig;
        }
        if(parseInt(rangediff) > parseInt(rangeRecordsConfig_final) ){
            //alert("To Records Should be Greater than From Records");
            return false;
        } else {
            return true;
        }
    } 
    
        
    function getSelections() {
        var checkedrecords;
        if($("#checkeddunsid").val() == '') {
            checkedrecords = ""; 
        } else {
            checkedrecords = $("#checkeddunsid").val();
        }
        var allVals = [];
        $("input[type=checkbox]:checked").each(function() {
            allVals.push($(this).val());
        });
        //var checkedRecord = $("input[type=checkbox]:checked").length;
        var build_type = $("input[name='download_records_type']:radio:checked").val();
        //if(checkedRecord == 0 && build_type == "checked_records") {
        if(build_type == "range_records") {
            $("#lightboxprofile, #lightbox-profile").fadeIn(300);
            return false;
        }
        else if(checkedrecords == '' && build_type == "checked_records") {
            alert("Select at least 1 company to Build Files");
            return false;
        } else {
            var duns_ids = allVals.join(',');
            $('#duns_ids').val(duns_ids);
            //window.document.frmSelectdownloadrecordtype.submit();
            if(build_type == "all_records"){
                
                var rangeRecordsConfig = "2000";

                var allrecords = "";
                var maxRecordslimit = "";
                //alert(maxRecordslimit);
                //alert(rangeRecordsConfig);
                if(parseInt(maxRecordslimit) < parseInt(rangeRecordsConfig)){
                    var rangeRecordsConfig_final = maxRecordslimit;
                    var recordlimit_max = 1;
                } else {
                    var rangeRecordsConfig_final = rangeRecordsConfig;
                    var recordlimit_max = 2;
                }
                if(parseInt(allrecords) > parseInt(rangeRecordsConfig_final)){
                    if(recordlimit_max == 2){
                        var retVal = confirm("Your Download Limit in single Download is "+rangeRecordsConfig_final+" Records,Do you want to continue ?");
                    } else {
                        var retVal = confirm("Your Download Limit is "+rangeRecordsConfig_final+" Records,Do you want to continue ?");   
                    }
                    
                    if( retVal == true ){
                        return true;
                    }else{
                        return false;
                    }
                } else {
                    window.document.frmSelectdownloadrecordtype.submit(); 
                }
            } else {
                window.document.frmSelectdownloadrecordtype.submit();
            }
        }
    }
    
