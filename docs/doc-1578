    function customCheckboxByID(id){
        customCheckbox('#'+id);
    }

    function customCheckbox(checkboxName){
        // check if the passed in param is ID or NAME
        if (checkboxName.indexOf('#') == 0) {
            var checkBox = $(checkboxName);
        } else {
            var checkBox = $('input[name="'+ checkboxName +'"]');
        }
        $(checkBox).each(function(){
            if($(this).parent().attr("class") != 'custom-checkbox'){
                $(this).wrap( "<span class='custom-checkbox'></span>" );
                if($(this).is(':checked')){
                    $(this).parent().addClass("selected");
                    $(this).parent().next('label').addClass("selected");
                }
            }
        });

        $(checkBox).click(function(){
            if(!$(this).parent().hasClass('disabled')){
                $(this).parent().toggleClass("selected");
                $(this).parent().next('label').toggleClass("selected");
            }
        });
    }

    function setCustomCheckbox(element, status) {
        element.attr("checked",status);
        if (status == true) {
            element.parent().addClass("selected");
            element.parent().next('label').addClass("selected");
        } else {
            element.parent().removeClass("selected");
            element.parent().next('label').removeClass("selected");
        }
    }

    function customRadio(radioName){
        var radio = $('input[name="'+ radioName +'"]');
        $(radio).each(function(){
            $(this).wrap( "<span class='custom-radio'></span>" );
            if($(this).is(':checked')){
                $(this).parent().addClass("selected");
                $(this).parent().next('label').addClass("selected");
            }
        });

        $(radio).click(function(){
            $(radio).each(function(){
                if($(this).is(':checked')){
                    $(this).parent().addClass("selected");
                    $(this).parent().next('label').addClass("selected");
                } else {
                    $(this).parent().removeClass("selected");
                    $(this).parent().next('label').removeClass("selected");
                }
            });
        });
    }

    function setCustomRadio(element, status) {
        setCustomCheckbox(element, status);
    }

    function customTextBox2(textBoxName){
        //get default value
        var textBoxValue = $("input[name='"+textBoxName+"']").val();
       
       if($("input[name='"+textBoxName+"']").css("display") != 'none'){
            $("input[name='"+textBoxName+"']").addClass("invalid");
        }
        
        //set input text empty on focus
        $(document).on("focus","input[name='"+textBoxName+"']",function(){
            if(this.value == textBoxValue){
                $(this).val('');
                $(this).css("color","#333");
                $(this).removeClass("invalid");
            }
        });
        // var buttonClicked = false;//check if button Add to Criteria is clicked
        // $("div[name='button']").hover(
        //     function () { buttonClicked = true; },
        //     function () { buttonClicked = false; }
        // );
        //restore input text with default value on focusout if no new value entered
        $(document).on("focusout","input[name='"+textBoxName+"']",function(){
            // if($(this).val() == '' && !buttonClicked){
            if($(this).val() == ''){    
                $(this).val(textBoxValue);
                $(this).css("color","#bfbfbf");
                $(this).addClass("invalid");
            }
        });
    }

    function customDropdown(dropdownId){
        //alert($('#ddl_states_county').height());
        var ddlWrapId = '#'+dropdownId+'_wrap';
        var dropdownId = '#'+dropdownId;
        var selectedInputId = '';
        var selectedTextId = '';

        $(document).on("click",ddlWrapId,function(){
            selectedInputId = '#'+$(ddlWrapId).find('input').attr('id');
            selectedTextId = '#'+$(ddlWrapId).find('span').attr('id');
            if($(dropdownId).height() == 800){
               $(dropdownId).css("top","-580px"); 
            } else if($(dropdownId).height() < 800 && $(dropdownId).height() > 600){
                $(dropdownId).css("top","-400px");
            } else if($(dropdownId).height() < 600 && $(dropdownId).height() > 400){
                $(dropdownId).css("top","-200px");
            } else if($(dropdownId).height() < 400 && $(dropdownId).height() > 200){
                $(dropdownId).css("top","-100px");
            }
            $(dropdownId).slideToggle();
        });
        
        $(document).on("click",ddlWrapId+" ul li",function(){
            if($(this).find('input').attr('name') === $(ddlWrapId).find('ul li input').attr('name')){
                $(selectedInputId).val($(this).find('input').val());
                var stateCounty = $(selectedInputId).val().split('#');
                $(selectedTextId).text(stateCounty[0]);
                $(selectedTextId).css("color","#333");
                $(this).parent().find('li.selected').removeClass('selected');
                $(this).addClass('selected');
            }
        });

        $('body').click(function(e){
            var clickedOn = $(e.target);
            if (!clickedOn.parents().andSelf().is(ddlWrapId)){
                $(dropdownId).hide('200');
            }
         });
    }

    function customSubTab(tabName){
        var tab = $('div[name="'+ tabName +'"]');
        $(tab).each(function(){
            if($(this).find('input').is(':checked')){
                $(this).addClass("sub_tab_active");
            }
        });

        $(tab).click(function(){
           $(tab).each(function(){
                if($(this).find('input').is(':checked')){
                    $(this).addClass("sub_tab_active");
                } else {
                    $(this).removeClass("sub_tab_active");
                }
            });
        });
    }

    function setDefaultCustomSubTab(tabName){
       var tab = $('div[name="'+ tabName +'"]'); 
       $("div[name='"+ tabName +"'] input:first").attr('checked', 'checked');  
       $(tab).each(function(){
            if($(this).find('input').is(':checked')){
                $(this).addClass("sub_tab_active");
                chooseDunsnosearch($(this).find('input:checked').val());
                chooseSicsearch($(this).find('input:checked').val());
                chooseNaicsearch($(this).find('input:checked').val());
                chooseHicsearch($(this).find('input:checked').val());
            } else {
                $(this).removeClass("sub_tab_active");
            }
        });
    }

    function customTreeCheckbox(checkboxName){
        var checkBox = $('input[name="'+ checkboxName +'"]');
        $(checkBox).each(function(){
            if($(this).parent().attr("class") != 'custom-checkbox' && $(this).parent().attr("class") != 'custom-checkbox selected'){
                $(this).wrap( "<span class='custom-checkbox'></span>" );
                if($(this).is(':checked')){
                    $(this).parent().addClass("selected");
                    $(this).parent().next('label').addClass("selected");
                }
            }
        });

    }

    function customTextBox(textBoxName){
        //get default value
        var textBoxValue = $("input[name='"+textBoxName+"']").val();
       
        $("input[name='"+textBoxName+"']").addClass("invalid");
        
        //set input text empty on focus
        $(document).on("focus","input[name='"+textBoxName+"']",function(){
                if(this.value == textBoxValue){
                    $(this).val('');
                    $(this).css("color","#333");
                }
                $(this).removeClass("invalid");
        });
        var buttonClicked = false;//check if button Add to Criteria is clicked
        $("div[name='button']").hover(
            function () { buttonClicked = true; },
            function () { buttonClicked = false; }
        );
        //restore input text with default value on focusout if no new value entered
        $(document).on("focusout","input[name='"+textBoxName+"']",function(){
            if($(this).val() == ''){
                $(this).val(textBoxValue);
                $(this).css("color","#bfbfbf");
                $(this).addClass("invalid");
            }
        });

        $(document).on("click","#clearSearch,div[name='button']",function(){
            $("input[name='"+textBoxName+"']").val(textBoxValue);
            $("input[name='"+textBoxName+"']").css("color","#bfbfbf");
            //$("input[name='"+textBoxName+"']").addClass("invalid");
        });
    }

    function customTextArea(textAreaId){
        var textAreaValue = $("#"+textAreaId).val();
        $(document).on("focus","#"+textAreaId,function(){
            if(this.value == textAreaValue){
                $(this).css("color","#525453");
                $(this).val('');
            }
        });
        $(document).on("focusout","#"+textAreaId,function(){
            if(this.value == ''){
                $(this).css("color","#AAAAAA");
                $(this).val(textAreaValue);
            }
        });
    }

    $(document).on("mouseover",".icon_jobOpenings, .icon_url, .active_radius, .inactive_radius, .icon_compareZipcode",function(){
        var tooltipWidth = $(this).find('div .tool-tip, .slideIn, .bottom').width();
        $(this).find('div .tool-tip, .slideIn, .bottom').css("margin-left","-"+(tooltipWidth/2+5)+"px");
    });
