                $(".trigger").toggleClass("active");

                $(".trigger").click(function(){
                    $(".panel").toggle("fast");
                    $(".panel_button").toggle("fast");
                    $(this).toggleClass("active");
                    return false;
                });

                $('form').bind('submit', function() {
                    //$.blockUI();
                    //$('#processing-image').show()
                });


                $("#company-email").click(function() {
                    $("#lightboxcompany, #lightbox-company").fadeIn(300);
                });
                $("a#closeRefCom").click(function(){
                    $("#lightboxcompany, #lightbox-company").fadeOut(300);
                });

                $("#saveCompany").click(function() {
                    $("#lightboxsave, #lightbox-save").fadeIn(300);
                });
                $("a#closeRefSave").click(function(){
                    $("#lightboxsave, #lightbox-save").fadeOut(300);
                });

                $("#saveCompanyEmailSubmit").click(function(){
                    var validator = $('#saveCompanyEmail').valid();
                    if(validator != true) {
                        return false;
                    }
                    var dataString = $("#saveCompanyEmail").serialize();;
                    $.ajax({
                        type: 'POST',
                        url: 'http://www.mergentintellect.com/index.php/search/addCompanyToList',
                        data: dataString,
                        success: function(message) {
                            $("#lightboxsave, #lightbox-save").fadeOut(300);
                            alert(message);
                        }
                    });
                });


                $("#companySubmit").click(function(){
                    var validator = $('#companyEmail').valid();
                    if(validator != true) {
                        return false;
                    }
                    var hooverid = $("#companyEmail").serialize();
                    $.ajax({
                        type: 'POST',
                        url: 'http://www.mergentintellect.com/index.php/export/emailCompanyDetails',
                        data: hooverid,
                        success: function(message) {
                            $("#lightboxcompany, #lightbox-company").fadeOut(300);
                            alert(message);
                        }
                    });
                });

            });


// JS from menu.php
    $(document).ready(function() {
        if ($("#buildfilesfor").val() == "search") {
            $('#checkeddunsid').val("");    
        } else if ($("#buildfilesfor").val() == "advancesearch") {
            $('#checkeddunsid').val("");
        } else if ($("#buildfilesfor").val() == "jobsearch") {
            $('#checkeddunsid').val("");
        } else if ($("#buildfilesfor").val() == "jobsearch_employer") {
            $('#checkeddunsid').val("");
        } else {
            $('#checkeddunsid').val("");
        }
        $.validator.addMethod("compRange", mycompRangeOnlyValidation, "To Records Should be Greater than From Records.");
        $.validator.addMethod("compRangeDiff", mycompRangeDiffValidation, "Difference should be lesser than 2000.");
        
        $.validator.addMethod("numbercommatxtarea", function(value, element)
        {
            return this.optional(element) || /^[0-9]+(,[0-9]+)*,?$/i.test(value.replace(/\s/g, ''));
        }, "Please enter a valid email address.");
        
        $("input[name='newlisting_status']:radio").click(function(){
            if ($("input[name='newlisting_status']:checked").val() == 'between') {
                $("#newlisting_enddate").show();
            } else {
                $("#newlisting_enddate").hide(); 
            }
        });
        $("input[name='yearestablished_status']:radio").click(function(){
            if ($("input[name='yearestablished_status']:checked").val() == 'between') {
                $("#yearestablished_enddate").show();
            } else {
                $("#yearestablished_enddate").hide(); 
            }
        });
        $("input[name='employee_status']:radio").click(function(){
            if ($("input[name='employee_status']:checked").val() == 'between') {
                $("#employees_endnumber").show();
                customTextBox('employees_endnumber');
            } else {
                $("#employees_endnumber").hide().find('input').removeClass('invalid'); 
