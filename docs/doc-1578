                        naichtml += "<tr><td>"+naicKeywordcodesnew_value[1]+"-"+naicKeywordcodesnew_value[0]+ "</td><td><a href='#' onclick='removeNaiccodes_basicsearch("+j+")'>Delete</a></td></tr>";
                        j++;
                    }
            }
             var naiccodeval_new = naicval_new.join("#");
             $('#naicKeywordcodes').val(naiccodeval_new);
        } else {
