        $("#sicKeyword_search").autocomplete("http://www.mergentintellect.com/index.php/advancesearch/autocompleteSickeyword", {
            width: 260,
            matchContains: true,
            selectFirst: true,
            minChars:2, 
            autoFill:false,
            mustMatch:false,
            cacheLength:10,
            max:10
        });
        

        
        $('#sicKeyword_search').result(function (event, data, formatted) {
            var sichtml = '';
            var siccode_keyword = new Array();
            sichtml = '<br /><table cellspacing=0 cellpadding=0 border=1 class=data_grid width=100%>';
            sichtml += '<tr><th>SIC Description</th><th>Action</th></tr>';
      
            $('#sicKeyword_search').val(''); 
            var sicKeywordcodes= $('#sicKeywordcodes').val();
            if(sicKeywordcodes == '') {
                $('#sicKeywordcodes').val(data);
            } else if(sicKeywordcodes != '') {
                var sicKeywordcodesoldval = $('#sicKeywordcodes').val();
                var sicKeywordcodesnewval = sicKeywordcodesoldval +"#"+data;
                $('#sicKeywordcodes').val(sicKeywordcodesnewval);
            }
            var sicKeywordcodesnew= $('#sicKeywordcodes').val();
            var sicKeywordcodesnew_val = sicKeywordcodesnew.split("#");
            for(var i=0;i< sicKeywordcodesnew_val.length;i++){
                var sicKeywordcodesnew_value = sicKeywordcodesnew_val[i].split("  ");
                //alert(sicKeywordcodesnew_value[1]);
                siccode_keyword[i] = sicKeywordcodesnew_value[0];
                sichtml += "<tr><td>"+sicKeywordcodesnew_value[1]+ "-"+sicKeywordcodesnew_value[0]+"</td><td><a href='#' onclick='removeSiccodes_basicsearch("+i+")'>Delete</a></td></tr>";
            }
                        
            sichtml += '</table>'
            var siccode_keyword_final = siccode_keyword.join();
            $('#sicKeywordcodes_display').show();
            $('#sicKeywordcodes_display').html(sichtml);
            $('#siccode_keyword').val(siccode_keyword_final);
           
        });
        

                    
        // search Siccode Autosearch options starts New Code
        //Advance search Siccode Autosearch options starts
                    
        $("#sicKeyword").autocomplete("http://www.mergentintellect.com/index.php/advancesearch/autocompleteSickeyword", {
            width: 272,
            matchContains: true,
            selectFirst: false,
            minChars:2, 
            autoFill:false,
            mustMatch:false,
            cacheLength:0,
            max:10,
            extraParams:{starts_contains: function() { return $("#starts_contains").val(); }}
        });

        $("#peoplesicKeyword").autocomplete("http://www.mergentintellect.com/index.php/advancesearch/autocompleteSickeyword", {
            width: 272,
            matchContains: true,
            selectFirst: false,
            minChars:2, 
            autoFill:false,
            mustMatch:false,
            cacheLength:0,
            max:10,
