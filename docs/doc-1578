        //Advance search Naiccode Autosearch options Ends
        $("#changeAdvancesearchSetting").click(function(){
            var templateid = 'templateid=' + $("input[id='templateid']:checked").val();
            $.ajax({
                type: 'POST',
                url: 'http://www.mergentintellect.com/index.php/advancesearch/changeAdvancesearch',
                dataType: 'json',
                data: templateid,
                success: function(data) {
                    $("#changecriteriaMsg").html('<font color="green"><b>Advance Search Criterias Changed for Current search Successfully</b></font>');
                    $("#changecriteriaMsg").fadeIn(300);
                    $("#changecriteriaMsg").fadeOut(5000); 
                }
            });
        });
        
        // Pre-select any previously checked records on Search Results page
        var checkedrecords = $("#checkeddunsid").val();
        if(typeof checkedrecords != 'undefined' && checkedrecords != '') {
            var checkedrecordsSplit = checkedrecords.split(",");
            $("#checkedcount").html(checkedrecordsSplit.length);
            $.each(checkedrecordsSplit, function(index, value) {
                $("#chk"+ value).attr('checked','checked');
            }); 
        } else { 
            $("#checkedcount").html("0");
        }
        if($('#dbdunsid').length){
            var dbdunsid = $('#dbdunsid').val().split(",");
            var allchkstatus = new Array();
            for(i=0;i<dbdunsid.length;i++) {
                if ($('#chk'+dbdunsid[i]).attr('checked')) {
                    allchkstatus[i] = 1;
                } else {
                    allchkstatus[i] = 0; 
                }
            }
            var checkvalue = include(allchkstatus, 0);
            if(checkvalue == undefined) {
                $("#toggleChecked").attr('checked','checked');
            } else {
                $("#toggleChecked").attr('checked',false);
            }  
        }
        $('.search_results input:checkbox.hideInput').each(function() {
            customCheckbox($(this).attr('name'));
        })
        // Pre-select any previously checked records on Search Results page
    });
    
    function viewcensus_companydetail(census_type,census_value,census_value_wihoutencode){
        if(census_type == "zipcode"){
            var census_type_text = "Zip code";
        } else if(census_type == "msacode"){
            var census_type_text = "MSA code";
        }
        $("#lightboxprofile, #lightbox-censuschart").fadeIn(300);
        $.blockUI();
        $.ajax({
            type: 'POST',
            url: 'http://www.mergentintellect.com/index.php/advancesearch/getCensusdatabytype/'+census_type+'/'+census_value,
            dataType: 'json',
            success: function(data) {
                $.unblockUI(); 
                //alert(data['POPULATION']);
           if(data['POPULATION'] != '' && data['POPULATION'] != null){
          //Funnel Chart Code Starts Here
                        var chart = new Highcharts.Chart({
                            chart: {
                                renderTo: 'census',
                                backgroundColor: '#FFFFFF',
                                type: 'bar'
                            },
                            title: {
                                text: '2010 Census data for '+census_type_text+' ('+census_value_wihoutencode+')'
                            },
                            subtitle: {
                                text: 'Population and Household'
                            },
                            xAxis: {
                                categories: ['Population','Male', 'Female', 'White', 'Black', 'Hispanic', 'Asian', 'Hawaiian', 'Indian','Others','','Tot House','Person/House','Income/House'],
                                title: {
                                    text: null
                                }
                            },
                            yAxis: {
                                min: 0,
                                title: {
                                    text: 'Population and Household (Thousands)',
                                    align: 'high'
                                },
                                labels: {
                                    overflow: 'justify'
                                }
                            },
                            tooltip: {
                                enabled: true
                                //valueSuffix: 'millions'
                            },
                            plotOptions: {
                                bar: {
                                    dataLabels: {
                                        enabled: true
                                    }
                                }
                            },
                            legend: {
                                layout: 'vertical',
                                enabled: false,
                                align: 'right',
                                verticalAlign: 'top',
                                x: -100,
                                y: 100,
                                floating: true,
                                borderWidth: 1,
                                backgroundColor: '#FFFFFF',
                                shadow: true
                            },
                            credits: {
                                enabled: false
                            },
                            series: [{
                                name: 'census',
                                data: [parseInt(data['POPULATION']),parseInt(data['MALEPOPULATION']),parseInt(data['FEMALEPOPULATION']),parseInt(data['WHITEPOPULATION']),parseInt(data['BLACKPOPULATION']),parseInt(data['HISPANICPOPULATION']),parseInt(data['ASIANPOPULATION']),parseInt(data['HAWAIIANPOPULATION']),parseInt(data['INDIANPOPULATION']),parseInt(data['OTHERPOPULATION']),'',parseInt(data['HOUSEHOLDSPERZIPCODE']),parseInt(data['PERSONSPERHOUSEHOLD']),parseInt(data['INCOMEPERHOUSEHOLD'])]
                            }]
                        });
          
                //Funnel Chart Code Ends Here
           } else {
               $("#census").html('<font color="Red"><b>Census Data Not Found</b></font>');
           }       
                    }
        });
    }
    
    function removeSiccodes_basicsearch(sicarrval){
        //var sicKeywordcodesnew= $('#sicKeywordcodes').val();
        var sichtml;
        var siccode_keyword_delete = new Array();
        sicval_new = new Array();
        //sicval_new = '';
        sichtml = '<br><table cellspacing=0 cellpadding=0 border=1 class=data_grid width=100%>';
        sichtml += '<tr><th>SIC Description</th><th>Action</th></tr>';
        //alert(sicKeywordcodesnew);
        //alert(sicarrval);
        var sicKeywordcodesnew= $('#sicKeywordcodes').val();
        var sicKeywordcodesnew_val = sicKeywordcodesnew.split("#");
        //alert(sicKeywordcodesnew_val.length);
        if(sicKeywordcodesnew_val.length > 1){
            var j=0;
            for(var i=0;i< sicKeywordcodesnew_val.length;i++){
                        if(parseInt(i) != parseInt(sicarrval)) {
                        //alert(sicKeywordcodesnew_val[i]);
                        sicval_new[j] = sicKeywordcodesnew_val[i];
                        
                        var sicKeywordcodesnew_value = sicKeywordcodesnew_val[i].split("  ");
                        siccode_keyword_delete[j] = sicKeywordcodesnew_value[0];
                        sichtml += "<tr><td>"+sicKeywordcodesnew_value[1]+"-"+sicKeywordcodesnew_value[0]+ "</td><td><a href='#' onclick='removeSiccodes_basicsearch("+j+")'>Delete</a></td></tr>";
                        j++;
                    }
            }
             var siccodeval_new = sicval_new.join("#");
             $('#sicKeywordcodes').val(siccodeval_new);
        } else {
            sichtml = '';
            $('#sicKeywordcodes').val('');
        }
        sichtml += '</table>'
        
        var siccode_keyword_final = siccode_keyword_delete.join();
        $('#sicKeywordcodes_display').show();
        $('#sicKeywordcodes_display').html(sichtml);
        $('#siccode_keyword').val(siccode_keyword_final);
  
    }  
    
    function removeNaiccodes_basicsearch(naicarrval){
        //var sicKeywordcodesnew= $('#sicKeywordcodes').val();
        var naichtml;
        var naiccode_keyword_delete = new Array();
        naicval_new = new Array();
        //sicval_new = '';
        naichtml = '<br><table cellspacing=0 cellpadding=0 border=1 class=data_grid width=100%>';
        naichtml += '<tr><th>NAICS Description</th><th>Action</th></tr>';
