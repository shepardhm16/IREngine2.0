        if(naicKeywordcodesnew_val.length > 1){
            var j=0;
            for(var i=0;i< naicKeywordcodesnew_val.length;i++){
                        if(parseInt(i) != parseInt(naicarrval)) {
                        //alert(sicKeywordcodesnew_val[i]);
                        naicval_new[j] = naicKeywordcodesnew_val[i];
                        var naicKeywordcodesnew_value = naicKeywordcodesnew_val[i].split("  ");
