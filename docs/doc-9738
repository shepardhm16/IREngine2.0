function FrontPage_Form1_Validator(theForm)
{

  if (theForm.Classof.selectedIndex < 0)
  {
    alert("Please select one of the \"Classof\" options.");
    theForm.Classof.focus();
    return (false);
  }

  var radioSelected = false;
  for (i = 0;  i < theForm.Econ.length;  i++)
  {
    if (theForm.Econ[i].checked)
        radioSelected = true;
  }
