							}
 }
 
 
function mbSetRoomDate(){	
							var spnBtnMarantzNeedRm = document.getElementById("SetRoomDate")
							// test: alert("spnBtnMarantzNeedRm to string: " + spnBtnMarantzNeedRm.toString());
							// test 10/14/10: alert("mbSetRoomDate: Btn Inner HTML= " + spnBtnMarantzNeedRm.innerHTML);
								 							
              if (spnBtnMarantzNeedRm.innerHTML == "Schedule Room"){
                 spnBtnMarantzNeedRm.innerHTML = "Cancel date";
						  RoomDate.style.display = "inline";
						  RoomDate.enabled = true;
					  } else {
						  spnBtnMarantzNeedRm.innerHTML = "Schedule Room";
						  RoomDate.style.display = "none";
							}				 
}
