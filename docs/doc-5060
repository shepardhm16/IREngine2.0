function mbMarantzChecked(){
		var chkboxMarantz = document.getElementById("ID0EZEAC");
		// ID above from: input name="cgrpAudio" id="ID0EZEAC" type="checkbox" ektdesignns_nodetype="item" onclick="mbMarantzChecked
		//delete 10/14/10 alert("chkboxMarantz to string: " + chkboxMarantz.toString());
