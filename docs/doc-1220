			/**
			 * QuerySpy recording for LibAnswers
			 * @param {Object} config
			 * @param {String} [config.domain] LA domain (unnecessary in LA itself)
			 * @param {Int} [config.qlog_id] current query spy ID if there is one already
			 * @param {Int} [config.group_id] current LA group id if applicable
			 */
			springSpace.querySpy_ = function(config) {
				this.domain = (config.domain) ? config.domain: '';
				if (this.domain !== '') { this.domain = '//'+this.domain; }
				this.qlog_id = (config.qlog_id) ? config.qlog_id : 0;
				this.group_id = (config.group_id) ? config.group_id : 0;
				this.first_search_saved = false;
				this.query = '';
				
				/**
				 * record a query spy transactions
				 * @param {Object} data
				 * @param {String} data.query search string
				 */
				this.record = function(data) {
					if (this.first_search_saved) {
						this.first_search_saved = false;
						if (this.query == data.query) {
							return;
						}
					}
					if (!data.query || this.query == data.query) { return; }
					if (!data.refer) { data.refer = location.href; }
					data.group_id = this.group_id;
					data.qlog_id = this.qlog_id;
					this.query = data.query;
					
					var self = this;
					jQuery.post(
						self.domain+'/process_public.php?m=qlog',
						data,
						function(d) {
							self.qlog_id = d.qlog_id;	
						},
						'json'
					);
				};
				/**
				 * Goto an FAQ, recording query spy on the way
				 * @param {Int} data.faqid target FAQ id
				 */
				this.gotoFAQ = function(data) {
					if (!data.faqid) {
						//console.log('nofaqid');	
						return;
					}
					var self = this;
					jQuery.ajax({
						type: 'POST',
						url: self.domain+'/process_public.php?m=gotoa',
						data: { faqid: data.faqid, qlog: self.qlog_id },
						success: function (data) {
							//console.log(data);
							return true;
						}
					});
				};
			};
			springSpace.querySpy = new springSpace.querySpy_({ qlog_id: springSpace.la.Page.qlog_id, group_id: springSpace.la.Page.group_id });
			if (springSpace.la.Page.qlog_id > 0) {
				springSpace.querySpy.first_search_saved = true;
				springSpace.querySpy.query = "";
			}
			
			//we need to set the object so its in scope for the History event
			var searchcontroller = {};			
			
			jQuery(document).ready(function(){
				
				//add the search header to the top column
				jQuery('<div>').append( jQuery('<h2>').addClass('s-srch-top-header') ).prependTo( jQuery('#s-la-page-column-0') );
				
				//set the selectors for controller elements
				var search_box_id = jQuery('.s-la-box-type-5').attr('id');
				search_conf.selector = '#'+search_box_id + ' .s-la-box-content';
				search_conf.header_selector = '#s-la-page-column-0 .s-srch-top-header';
				
				//instantiate the controller
				searchcontroller = new SearchController(search_conf);
				searchcontroller.loadSources();
				searchcontroller.retrieve();

				//If question form is present, set the query in the question field
				$('input[name=pquestion]').val("");
			});
						
		</script> 
