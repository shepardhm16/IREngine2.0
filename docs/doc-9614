        <div id="station-list" class="row collapse has-sidebar station-list"> 
         <div class="columns"> 
          <table id="station-list-table" class="tablesaw tablesaw-swipe tablesaw-sortable data-table" data-tablesaw-mode="swipe" data-tablesaw-sortable data-tablesaw-minimap> 
           <thead> 
            <tr> 
             <th scope="col" data-tablesaw-sortable-col data-tablesaw-sortable-default-col data-tablesaw-priority="persist">Station Location</th> 
             <th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="persist">Temp.</th> 
             <th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="1">Windchill</th> 
