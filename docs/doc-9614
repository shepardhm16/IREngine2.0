        <script type="text/template" id="moon-phase-template">
	<td>
		<%  var moondir = "";
			var title = ""; 
			if(loc.latitude < 0) {
				moondir = "south";
			} else {
				moondir = "north";
			}
			if(today.moonrise.date.year == localtime.year && today.moonrise.date.month == localtime.month && today.moonrise.date.day == localtime.day) {
				title = labels.today.label;
			} else {
				title = today.moonrise.date.monthname_short + ' ' + today.moonrise.date.day;
			}
		%>
		<span><%= title %></span>
		<div class="<%= moondir %> moon<%= today.moon_age %>"></div>
		<%= today.moon_phase %>
	</td>
	<% _.each(moonDays, function(day) {  %>
		<td>
		<span><%= day.date.monthname_short + ' ' + day.date.day %></span>
		<div class="<%= moondir %> moon<%= day.moon_age %>"></div>
		<%= day.moon_phase %>
		</td>
	<% }); %>
	</script> 
