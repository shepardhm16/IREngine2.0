		<td>Low</td>
		<td>
		<span class="wx-value"> - </span>
		</td>
		<td>
		<span class="wx-value"><%= dew.avg.low %></span>
		<span class="wx-unit"><%= tempUnit %></span>
		</td>
		<td>
		<span class="wx-value"><%= dew.range.low.min %></span>
		<span>to</span>
		<span class="wx-value"><%= dew.range.low.max %></span>
		<span class="wx-unit"><%= tempUnit %></span>
		</td>
		</tr>
		<tr>
		<td>High</td>
		<td>
		<span class="wx-value"> - </span>
		</td>
		<td>
		<span class="wx-value"><%= dew.avg.high %></span>
		<span class="wx-unit"><%= tempUnit %></span>
		</td>
		<td>
		<span class="wx-value"><%= dew.range.high.min %></span>
